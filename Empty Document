sudo docker image rm -f mysql
sudo docker network create my-network
docker pull mysql
sudo docker run -d --name mysql-server --network my-network -e MYSQL_ROOT_PASSWORD=root mysql
sudo docker run -it --rm --network my-network mysql sh -c 'exec mysql -h"mysql-serv" -P"3306" -uroot -p"root"'




sudo docker run -it -p 8000:80 --rm --network my-network acts
